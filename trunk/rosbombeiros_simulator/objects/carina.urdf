	<!--controllers (SICK SENSOR PARALLEL) -->
	<gazebo reference="sick_parallel_laser_link">
		<sensor:ray name="sick_parallel_laser">
			<rayCount>21</rayCount>
			<rangeCount>21</rangeCount>
			<laserCount>1</laserCount>

			<origin>0.0 0.0 0.0</origin>
			<displayRays>line</displayRays>

			<minAngle>-85.0</minAngle>
			<maxAngle>85.0</maxAngle>

			<minRange>0.01</minRange>
			<maxRange>6.00</maxRange>
			<resRange>0.01</resRange>
			<updateRate>10.0</updateRate>

			<controller:gazebo_ros_laser name="gazebo_ros_sick_laser_controller"
				plugin="libgazebo_ros_laser.so">
				<hokuyoMinIntensity>101</hokuyoMinIntensity>
				<gaussianNoise>0.005</gaussianNoise>
				<alwaysOn>true</alwaysOn>
				<updateRate>20.0</updateRate>
				<topicName>scan_front</topicName>
