<?xml version="1.0" ?>
<launch>
        
    <node pkg="pr2_teleop02" type="teleop_pr2_keyboard" name="spawn_teleop_keyboard" output="screen">
      <!-- <remap from="cmd_vel" to="carina_controller/command"/> -->
      <remap from="cmd_vel" to="carina_controller/cmd_vel" />
      <!-- el doble -->
      <param name="walk_vel" value="0.8" />
      <param name="run_vel" value="1.5" />
      <param name="yaw_rate" value="0.5" />
      <param name="yaw_run_rate" value="1.5" />
    </node>
    
</launch>
