	<gazebo>
		<controller:gazebo_ros_time
			name="gazebo_ros_time"
			plugin="libgazebo_ros_time.so"
		>
			<alwaysOn>true</alwaysOn>
			<updateRate>1000.0</updateRate>
			<interface:audio name="dummy_gazebo_ros_time_iface_should_not_be_here" />
		</controller:gazebo_ros_time>

		<!-- PR2_ACTARRAY -->
		<controller:gazebo_ros_controller_manager
			name="gazebo_ros_controller_manager"
			plugin="libgazebo_ros_controller_manager.so"
		>
			<alwaysOn>true</alwaysOn>
			<updateRate>1000.0</updateRate>
			<interface:audio name="gazebo_ros_controller_manager_dummy_iface" />
		</controller:gazebo_ros_controller_manager>
	</gazebo>

	<!--controllers (SICK SENSOR PARALLEL) -->

	<gazebo reference="sick_parallel_laser_link">
		<sensor:ray name="sick_parallel_laser">
			<rayCount>640</rayCount>
			<rangeCount>640</rangeCount>
			<laserCount>1</laserCount>

			<origin>0.0 0.0 0.0</origin>
			<displayRays>fan</displayRays>

			<minAngle>-90.0</minAngle>
			<maxAngle>90.0</maxAngle>

			<minRange>0.05</minRange>
			<maxRange>80.1</maxRange>
			<resRange>0.001</resRange>
			<updateRate>10.0</updateRate>
			<controller:gazebo_ros_laser
				name="gazebo_ros_sick_laser_controller"
				plugin="libgazebo_ros_laser.so"
			>
				<gaussianNoise>0.005</gaussianNoise>
				<alwaysOn>true</alwaysOn>
				<updateRate>10.0</updateRate>
				<topicName>scan_front</topicName>
				<!--topicName>base_scan</topicName -->
				<frameName>sick_parallel_laser_link</frameName>
				<interface:laser name="gazebo_ros_sick_laser_iface" />
			</controller:gazebo_ros_laser>
		</sensor:ray>
	</gazebo>

	<!--controllers (SICK SENSOR INCLINED) -->

	<gazebo reference="sick_inclined_laser_link">
