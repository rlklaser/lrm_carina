<!-- RoBombeiros -->
	<gazebo reference="sick_parallel_laser_link">
		<material>Gazebo/Black</material>
		<turnGravityOff>false</turnGravityOff>
		<sensor:ray name="sick_parallel_laser">
			<rayCount>21</rayCount>
			<rangeCount>21</rangeCount>
			<laserCount>1</laserCount>
			<origin>0.0 0.0 0.0</origin>
			<displayRays>lines</displayRays>
			<minAngle>-90.0</minAngle>
			<maxAngle>90.0</maxAngle>
			<minRange>0.05</minRange>
			<maxRange>30.1</maxRange>
			<resRange>0.001</resRange>
			<updateRate>10.0</updateRate>
			<controller:gazebo_ros_laser
				name="gazebo_ros_sick_laser_controller"
				plugin="libgazebo_ros_laser.so"
			>
				<hokuyoMinIntensity>100</hokuyoMinIntensity>
				<gaussianNoise>0.005</gaussianNoise>
				<alwaysOn>true</alwaysOn>
				<updateRate>10.0</updateRate>
				<topicName>base_scan</topicName>
				<frameName>laser_link</frameName>
				<interface:laser name="gazebo_ros_sick_laser_iface" />
			</controller:gazebo_ros_laser>
		</sensor:ray>
	</gazebo>

	<gazebo reference="camera_base_link">
		<material>Gazebo/Red</material>
		<turnGravityOff>false</turnGravityOff>
		<sensor:camera name="camera_sensor">
			<imageSize>640 480</imageSize>
			<imageFormat>R8G8B8</imageFormat>
			<hfov>90</hfov>
			<nearClip>0.1</nearClip>
			<farClip>100</farClip>
			<updateRate>15.0</updateRate>
			<controller:gazebo_ros_camera
				name="camera_base_link_controller"
				plugin="libgazebo_ros_prosilica.so"
			>
				<alwaysOn>true</alwaysOn>
				<updateRate>30.0</updateRate>
				<cameraName>camera</cameraName>
				<imageTopicName>image_raw</imageTopicName>
				<cameraInfoTopicName>camera_info</cameraInfoTopicName>
				<pollServiceName>request_image</pollServiceName>
				<frameName>camera_optical_link</frameName>
				<CxPrime>320</CxPrime>
				<Cx>320</Cx>
				<Cy>240</Cy>
				<hackBaseline>0</hackBaseline>
				<focalLength>320</focalLength>
				<distortionK1>0</distortionK1>
				<distortionK2>0</distortionK2>
				<distortionK3>0</distortionK3>
				<distortionT1>0</distortionT1>
				<distortionT2>0</distortionT2>
			</controller:gazebo_ros_camera>
		</sensor:camera>
	</gazebo>

	<gazebo>
			
		<controller:gazebo_ros_controller_manager
			name="gazebo_ros_controller_manager"
			plugin="libgazebo_ros_controller_manager.so"
		>
