<?xml version="1.0" ?>
<gazebo version="1.2">
	<world name="default">

		<plugin
			name="carina_world_plugin"
			filename="libcarina_world_plugin.so" />
			
        <!-- A global light source -->
		<include>
			<uri>model://sun</uri>
		</include>

        <!-- A ground plane -->
		<include>
			<uri>model://ground_plane</uri>
		</include>
	</world>
</gazebo>